func fact(n) {
    r = 1
    del = n + 1
    for i in range(1, n + 2) {
        r = r * i
    }!
    return r / del
};

func abs(n) {
    if (n < 0) {
        return n * -1
    } else {
        return n
    }:
};

func pow(n, s) {
    r = n^s
    return r
};

func sqrt(n) {
    r = eval(str(n) + "** 0.5")
    return r
};

func pi() {
    return 3.14
};





